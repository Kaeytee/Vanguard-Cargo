{
  "name": "mainapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate-seo && tsc -b && vite build",
    "generate-seo": "node scripts/generate-seo-files.cjs",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "npm run build && vercel",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "package:auto-move": "npx ts-node src/utils/runPackageAutoMove.ts",
    "package:status": "npx ts-node -e \"import('./src/utils/runPackageAutoMove.js').then(m => m.getProcessingStatus())\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@tailwindcss/vite": "^4.1.8",
    "@tanstack/react-query": "^5.87.4",
    "@types/qrcode": "^1.5.5",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "libphonenumber-js": "^1.12.9",
    "lucide-react": "^0.511.0",
    "qrcode": "^1.5.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-google-recaptcha": "^3.1.0",
    "react-i18next": "^15.6.0",
    "react-icons": "^5.5.0",
    "react-phone-number-input": "^3.4.12",
    "react-router": "^7.6.1",
    "react-router-dom": "^7.9.1",
    "sweetalert2": "^11.22.1",
    "tailwind-merge": "^3.3.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.5",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  }
}
